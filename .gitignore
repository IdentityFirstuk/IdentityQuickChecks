# ============================================================================
# PowerShell Gallery .gitignore
# ============================================================================
# This file specifies intentionally untracked files that should be ignored
# for PowerShell Gallery submission.

# Build outputs
bin/
obj/
*.dll
*.exe
*.pdb
*.user
*.suo

# Package outputs
*.nupkg
*.zip
build/
dist/
release/

# Test outputs
TestResults/
coverage/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# PowerShell specific
*.psess
*.psproj
*.user
.vs/

# Certificate files (never commit these)
*.pfx
*.cer
*.pem
*.key

# Temporary files
*.tmp
*.bak
*.orig
*.rej

# CI/CD
.github/workflows/*.yml
azure-pipelines.yml

# Documentation build
docs/_site/
docs/.build/

# External dependencies
packages/
node_modules/

# OS files
Thumbs.db
ehthumbs.db

# Logs
*.log
logs/

# Environment files
.env
*.local
